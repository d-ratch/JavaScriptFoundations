    <script>
        // Addition operator
        let x = 100 + 50;

        let a = 100
        let b = 857

        // Simple function via addition
        let c = a + b;

        let d = 5
        
        // Simple Expression
        let t = (100 + 685) * a

        // Remainder operator
        let r = b % a

        // Increment
        r++;

        // Decrement
        r--;

        // Exponentiation
        let p = d ** 3;

        // Exponentiation II
        let q = Math.pow(d,3);

        // PEMDAS
        let n = 100 + 50 * 3;
        // PEMDAS, but parentheses defines precedence
        let m = (100 + 50) * 3;

        // If two expressions hold the same weight, it is read from left to right
        let l = 100 + 50 - 3;

        let h = 100 / 50 * 3;

        alert(q);
    </script>
